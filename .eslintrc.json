{
  "rules": {
    "block-scoped-var": 1,
    "brace-style": [1, "1tbs"],
    "camelcase": 1,
    "comma-dangle": [1, "always-multiline"],
    "consistent-return": 1,
    "curly": 1,
    "eol-last": 1,
    "eqeqeq": 2,
    "no-nested-ternary": 1,
    "padded-blocks": 0,
    "quotes": 0,
    "no-cond-assign": 2,
    "no-constant-condition": 2,
    "no-duplicate-case": 2,
    "no-dupe-keys": 2,
    "no-empty-character-class": 2,
    "no-empty": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 1,
    "no-invalid-regexp": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": 2,
    "semi": [2, "always"],
    "space-before-function-paren": [1, "never"],
    "space-before-blocks": [1, "always"],
    "use-isnan": 2,
    "valid-typeof": 2
  },
  "globals": {
    "chrome": false,
    "angular": false
  }
}
